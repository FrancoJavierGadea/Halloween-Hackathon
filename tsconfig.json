{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@layouts/*": ["src/layouts/*"],
			"@assets/*": ["src/assets/*"],
			"@react/*": ["src/react/*"],
			"@cloudinary/*": ["src/cloudinary/*"],
			"@/*": ["src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}